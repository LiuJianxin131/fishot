/*图片裁剪相关*/
button{
  margin: 0;
  border: none;
  padding: 0;
}

#cover-preview{
  width: 100%;
  height: 100%;
  background-color: black;
}
#croper{
  width: 750rpx;
  height: 526rpx;
  position: absolute;
  top: 400rpx;
  left: 0;
  background-color: rgba(135,206,250,0.5);
  
}

#croper-sure{
  width: 120rpx;
  height: 50rpx;
  border-radius: 10rpx;
  color: black;
  background-color: rgba(135,206,250,0.8);
  font-size: 40rpx;
  position: absolute;
  top: 946rpx;
  right: 10rpx;
  text-align: center
}
#croper-cancel{
  width: 120rpx;
  height: 50rpx;
  border-radius: 10rpx;
  color: black;
  background-color: rgba(135,206,250,0.8);
  font-size: 40rpx;
  position: absolute;
  top: 946rpx;
  right: 150rpx;
  text-align: center
}

.editing-words-cover0{
  width: 600rpx;
  height: 350rpx;
  background-color: white;
  border-radius: 20rpx;
  position: absolute;
  top: 350rpx;
  left: 50%;
  margin-left: -300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  z-index: 10000;
}.editing-words-cover{
  width: 600rpx;
  height: 550rpx;
  background-color: white;
  border-radius: 20rpx;
  position: absolute;
  top: 350rpx;
  left: 50%;
  margin-left: -300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  z-index: 10000;
}
.name-text{
  color: black;
  font-size: 40rpx;
}
.me-info-img{
  width: 150rpx;
  height: 150rpx;
  border-radius: 75rpx;
}
.ph-color{
  font-family: Pingfang-SC-Medium;  
  color: #a1a1a1
}
.pic-edit-name{
  color: #a1a1a1;
  font-size: 26rpx;
  font-weight: 100; 
  font-family: Pingfang-SC-Medium;
  width: 490rpx;
  height: 60rpx;
  border-radius: 10rpx;
  background-color: #f7f7f7;
  box-sizing: border-box;
  padding: 0 10rpx;
}
.pic-edit-describe{
  color: #a1a1a1;
   font-weight: 100; 
  font-size: 26rpx;
  font-family: Pingfang-SC-Medium;
  width: 490rpx;
  height: 120rpx;
  border-radius: 10rpx;
  word-break: break-all;
  background-color: #f7f7f7;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  box-sizing: border-box;
  padding: 8rpx 10rpx;
}
.submit-cancel-container{
  width: 500rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.edit-cancel{
  width: 210rpx;
  height: 70rpx;
  background-color: #a1a1a1;
  color: white;
  border-radius: 40rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.edit-submit{
  width: 210rpx;
  height: 70rpx;
  background-color: #39bae8;
  color: white;
  border-radius: 40rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

#fi-story-filter{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 526rpx;
  /* filter: blur(2px); */
}

#fi-story-container{
  width: 100%;
  height: 526rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.3);
}
.fi-story-text{
  margin-top: 60rpx;
  margin-bottom: 40rpx;
  color: #ffffff;
  margin-left: -500rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.back-icon{
  width: 40rpx;
  height: 40rpx;
}

.back-text{
  height: 40rpx;
  font-size: 40rpx;
  color: #ffffff;
}

.change-pic-bt{
  width: 240rpx;
  height: 60rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: #ffffff;
  background-color: #00a0e9;
  border-radius: 30rpx;
  text-align:  center;
  line-height: 60rpx;
  margin-top: 100rpx;
}


.fi-story-name{
  color: #ffffff;
  font-size: 40rpx;
  font-weight: 400;
  font-family: Pingfang-SC-Bold;
  margin-top: 28rpx;
}
.fi-story-words{
  color: #ffffff;
  font-size: 24rpx;
  font-family: Pingfang-SC-Bold;
  margin-top: 25rpx;
  text-align: center;
  width: 480rpx;
  word-break: break-all
}
.fi-story-edit{
  width: 50rpx;
  height: 50rpx;
  margin-top: 30rpx;
}


/* 选色板 */
#color-bar{
  width: 100%;
  height: 150rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  /* box-sizing: border-box; */
  margin-top: 25rpx;
}
.color-bar-text{
  color: #2f2f2f;
  font-size: 32rpx;
  margin-left: 30rpx;
}
.color-bar-canvas{
  width: 100%;
  height: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  /* box-sizing: border-box; */
}
.color-circle{
  width: 48rpx;
  height: 48rpx;
  border-radius: 25rpx;
  position: relative;
}
.color-selected{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 0;
  bottom: 0;
}
.color-code0{
  background-color: #fe6e79;
}
.color-code1{
  background-color: #7329a2;
}
.color-code2{
  background-color: #00a0e9;
}
.color-code3{
  background-color: #00e362;
}
.color-code4{
  background-color: #fff45c;
}
.color-code5{
  background-color: #fd8930;
}
.color-code6{
  background-color: #fe303d;
}


/*相册编辑区  */
#pic-edit-container{
  width: 100%;
  height: auto;
  min-height: 600rpx;
  /* background-color: #ffffff; */
  margin-top: 20rpx;
  position: static;
  overflow: auto;
}
#timebar-canvas{
  position: absolute;
}
.pic-and-words{
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  background-color: rgba(0, 0, 0, 0);
}
.create-time{
  font-size: 25rpx;
  color: #a1a1a1;
  margin-left: 70rpx;
  margin-top: 4rpx;
}

.editing-pic{
  width: 100%;
  height: 320rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  margin-top: 10rpx;
  margin-bottom: 36rpx;
  
}
.arc-icon{
  width: 28rpx;
  height: 20rpx;
  margin-left: 15rpx;
  margin-top: 30rpx;
}
.head-icon{
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  margin-left: 15rpx;
}
.pic-words-container{
  width: 450rpx;
  height: 320rpx;
  margin-left: 20rpx;
  overflow: hidden; 
}
.pic-container{
   float: left; 
  width: 450rpx;
  height: 320rpx;
  /* background-color: blue; */
  transition: margin-left 0.5s ease;  background: url(data:image/png;base64,);

  background-size: cover;
  background-position: center center;
}
.words-container{
  float: left; 
  width: 450rpx;
  height: 320rpx;
  /* background-color: red; */
  transition: margin-left 0.5s ease;
  background: url(data:image/png;base64,);

  background-size: cover;
  background-position: center center;

}
.add-pic-container{
  width: 100%;
  height: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.insert-icon{
  width: 45rpx;
  height: 38rpx;
  margin-left: 25rpx;
}
.add-icon{
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}
.add-text{
  color: #4f4f4f;
  font-family: PingFang-SC-Medium;
  font-size: 30rpx;
  margin-left: 10rpx;
  letter-spacing: 0.2rpx;
}


